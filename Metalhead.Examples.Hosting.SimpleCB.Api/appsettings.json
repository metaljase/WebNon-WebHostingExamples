{
  "WeatherForecast": {
    "NumberOfDays": 7,
    "TemperatureScale": "C",
    "WindSpeedUnit": "MPH"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      },
      "FormatterName": "Simple",
      "FormatterOptions": {
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss ",
        "UseUtcTimestamp": true,
        "IncludeScopes": false,
        "SingleLine": false
      }
    }
  },
  "AllowedHosts": "*"
}
